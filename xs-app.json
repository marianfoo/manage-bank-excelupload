{
	"welcomeFile": "/",
	"authenticationMethod": "none",
	"routes": [
		{
			"source": "^/sap/bc/ui5_ui5",
			"destination": "XXX"
		},
		{
			"source": "^/sap/bc/ui2/app_index",
			"destination": "XXX"
		},
		{
			"source": "^/sap/opu/odata",
			"destination": "XXX"
		},
		{
			"source": "^/sap/bc/lrep",
			"destination": "XXX"
		},
		{
			"source": "^/resources/(.*)$",
			"destination": "sapui5",
			"target": "resources/$1"
		},
		{
			"source": "^/test-resources/(.*)$",
			"destination": "sapui5",
			"target": "test-resources/$1"
		},
		{
			"source": "^(.*)$",
			"target": "$1",
			"localDir": "./"
		}
	]
}
